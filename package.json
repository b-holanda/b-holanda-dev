{
  "name": "b-holanda.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest .vite/ssr-manifest.json --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest .vite/ssr-manifest.json --outDir dist/static && npm run build:server && node prerender",
    "serve": "NODE_ENV=production node server",
    "debug": "node --inspect-brk server"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "compression": "^1.7.5",
    "cssnano": "^7.0.6",
    "express": "^4.21.2",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "sass": "^1.83.0",
    "serve-static": "^1.16.2",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "lucide-vue-next": "^0.468.0",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  }
}
